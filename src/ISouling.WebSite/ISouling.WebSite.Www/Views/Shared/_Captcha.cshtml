@model string
@inject IViewLocalizer Localizer
<input name="@Model" id="@Model" size="6" />
<img src=" /Home/Captcha?_=@DateTimeOffset.Now.ToUnixTimeMilliseconds()" onclick="this.src = '/Home/Captcha?_=' + Date.now()" title="@Localizer["点击更换验证码"]" style="cursor: pointer;" alt="@Localizer["验证码"]" />
<input name="Captcha" id="Captcha" size="6" placeholder="@Localizer["请输入验证码"]" />
<button onclick="">@Localizer["获取验证码"]</button>